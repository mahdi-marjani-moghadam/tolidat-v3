{
  "name": "tolidat-v2",
  "version": "1.0.0",
  "description": "## Requirement > maximum php7.4",
  "main": "postcss.config.js",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npx tailwindcss -i ./src/css/style.css -o ./templates/template_tailwind/assets/css/style.css -w",
    "watch2": "tailwindcss -i ./src/css/style.css -o ./templates/template_tailwind/assets/css/style.css",
    "build": "postcss ./src/css/style.css -o ./templates/template_tailwind/assets/css/style.css",
    "build2": "npx tailwindcss -i ./src/css/style.css -o ./templates/template_tailwind/assets/css/style.css --minify",
    "build3": "set NODE_ENV=production",
    "prod": "NODE_ENV=production npx tailwindcss -i ./src/css/style.css -o ./templates/template_tailwind/assets/css/style.css --minify"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@gitlab.dabacenter.ir:10022/backend_php/tolidat-v2.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "caniuse-lite": "^1.0.30001441",
    "postcss-cli": "^9.1.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "autoprefixer": "^10.4.7",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.2.6",
    "tailwindcss-rtl": "^0.9.0"
  }
}
